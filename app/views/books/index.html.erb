<h2>Books</h2>
 <table>
   <%= image_tag @user.get_profile_image(100,100) %>
   <thead>
     <tr><% @books.each do |book| %> 
        <th></th>
        <th>Title</th>
        <td><%= link_to book.title, book_path(book.id) %></td>
        <th>Opinion</th>
        <th><%= book.body %></th>
      </tr>
         <% end %>
   </thead>
   <tbody></tbody>
   </table>
